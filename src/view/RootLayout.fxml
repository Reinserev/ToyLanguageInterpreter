<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.RootController">
   <center>
      <SplitPane prefHeight="160.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
          <Button mnemonicParsing="false" onAction="#exit" text="Exit" />
            <Button mnemonicParsing="false" onAction="#all_steps_pressed" text="AllSteps" />
            <Button mnemonicParsing="false" onAction="#handlePress" text="OneStep" />
        </buttons>
      </ButtonBar>
   </bottom>
   <top>
      <TextField fx:id="theLabel" alignment="CENTER" editable="false" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
